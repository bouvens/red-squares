{
  "name": "red-squares",
  "version": "1.0.0",
  "description": "React-redux game experiment with support of AI written on JS",
  "private": true,
  "scripts": {
    "build": "parcel build src/index.html --out-dir dist --public-url ./",
    "deploy": "gh-pages -d dist",
    "start": "parcel src/index.html --out-dir temp",
    "eslint": "eslint --ext .js,.jsx ./src",
    "eslint:html": "eslint --ext .js,.jsx -f html -o ./reports/eslint.checkstyle.html ./src",
    "eslint:fix": "eslint --fix --ext .js,.jsx  ./src"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.2.0",
    "state-control": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "babel-core": "^7.0.0-beta.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.6.0",
    "eslint-config-air": "^0.4.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^2.0.1",
    "less": "^3.9.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-dist": "^0.0.6",
    "postcss-modules": "^1.4.1"
  },
  "eslintConfig": {
    "extends": "air"
  },
  "author": "Alexander Demin <bouvens@gmail.com> (https://bouvens.github.io/)",
  "homepage": "https://github.com/bouvens/red-squares",
  "repository": "github:bouvens/red-squares",
  "license": "MIT"
}
