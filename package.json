{
  "name": "red-squares",
  "version": "1.0.0",
  "description": "React-redux game experiment with support of AI written on JS",
  "private": true,
  "scripts": {
    "build": "parcel build --target node src/index.html --out-dir dist",
    "deploy": "parcel build --target node src/index.html --out-dir dist && gh-pages -d dist",
    "start": "parcel src/index.html --out-dir temp"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.2.0",
    "state-control": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^2.0.1",
    "less": "^3.9.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-dist": "^0.0.6",
    "postcss-modules": "^1.4.1"
  },
  "author": "Alexander Demin <bouvens@gmail.com> (https://bouvens.github.io/)",
