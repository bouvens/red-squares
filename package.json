    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^2.0.1",
    "nwb": "0.23.x",
    "nwb-less": "^0.7.1"
